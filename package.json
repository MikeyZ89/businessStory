{
    "title": "Agency",
    "name": "startbootstrap-agency",
    "version": "6.0.3",
    "scripts": {
        "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
        "build:assets": "node scripts/build-assets.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js",
        "deploy": "gh-pages -d dist"
    },
    "description": "Agency is a one page HTML theme for Bootstrap.",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://mikezlot.com",
    "bugs": {
        "url": "https://github.com/MikeyZ89/businessStory",
        "email": "michael.zlotkowski@gmail.com"
    },
    "license": "MIT",
    "author": "businessStory",
    "repository": {
        "type": "git",
        "url": "https://github.com/MikeyZ89/businessStory"
    },
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@fortawesome/react-fontawesome": "^0.1.12",
        "bootstrap": "4.5.3",
        "gh-pages": "^3.1.0"
    },
    "devDependencies": {
        "autoprefixer": "10.0.1",
        "browser-sync": "2.26.13",
        "chokidar": "3.4.3",
        "concurrently": "5.3.0",
        "postcss": "8.2.10",
        "prettier": "2.1.2",
        "pug": "3.0.0",
        "sass": "1.28.0",
        "shelljs": "0.8.4"
    }
}
